#include<iostream>
using namespace std;

int main(){
  int n;
  int arr[n];
  cin>>n;
  for(i=0;i<n;i++)
  {
    cin>>arr[i];
  }
  
  counter=1;
  while(counter<n-1)
  {
      for(i=0;i<n-counter;i++)
      {
        if(arr[i]<arr[i+1])
        {
          int temp = arr[i];
          arr[i] = arr[i+1];
          arr[i+1] = temp;
        }
   }
   
   for(i=0;i<n;i++)
   {
      cout<<arr[i]<<" ";
   }
 return 0;
}
